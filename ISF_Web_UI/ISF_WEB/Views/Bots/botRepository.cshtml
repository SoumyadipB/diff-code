<div id="demoURLPopup" class="modal" role="dialog">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            @*<h4 class="modal-title well well-sm text-center"></h4>*@
        </div>
        <div class="modal-body" id="demoURLBody">
            <input type="hidden" id="rpaID" />
            Demo Video URL:
            <input type="text" id="videoURL" style="width:100%" readonly />
        </div>
        <div class="modal-footer">
            <button id="closeModal" type="button" class="btn btn-sm btn-default" data-dismiss="modal">Close</button>
        </div>
    </div>
</div>
<script>
    $('#demoURLPopup').on('show.bs.modal', function () {
        $(this).find('.modal-content').css({
            width: 'auto', //probably not needed
            height: 'auto', //probably not needed
            'max-width': '50%',
            'margin': 'auto'
        });
    });

</script>
