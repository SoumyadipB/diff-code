<div class="col-lg-12">
    <div class="panel panel-default">
        <div class="panel-heading">
            Bot Repository
        </div>

        <div class="panel-body" style="padding-bottom: 0px;padding-top: 10px;">

            <div class="row" style="padding-left: 0px;padding-bottom: 0px;padding-top: 5px;margin-bottom: 10px;">
                <div class="col-lg-10" style="margin-top: 0px;">
                    <div class="col-sm-1">
                        <label style="font-weight: bolder;font-size: 13px; margin-top:30px;"><nobr>Filter BOTs :</nobr></label>
                    </div>

                    <div class="col-sm-2" style="z-index: 0;margin-bottom: 0px;">
                        <label class="required">Domain/Subdomain:</label>
                        <select id="domainFilter" class="select2able" onchange="onChangeDomain()">
                            <option value=''>Select Domain/Subdomain</option>
                        </select>
                    </div>

                    <div class="col-sm-2" style="z-index: 0;margin-bottom: 0px;">
                        <label>Activity/Subactivity</label>
                        <select id="activityFilter" class="select2able" onchange="onChangeActivity()">
                            <option value=''>Select Activity/Subactivity</option>
                        </select>
                    </div>

                    <div class="col-sm-2" style="z-index: 0;margin-bottom: 0px;">
                        <label>Technology :</label>
                        <select id="techFilter" class="select2able" onchange="getTasks()">
                            <option value=''>Select Technology</option>
                        </select>
                    </div>
                    <div class="col-sm-2" style="z-index: 0;margin-bottom: 0px;">
                        <label>Tasks</label>
                        <select id="taskFilter" class="select2able">
                            <option value=''>Select Task</option>
                        </select>
                    </div>

                    <div class="col-sm-2" style="z-index: 0;margin-bottom: 0px;">
                        <label>Market Area :</label>
                        <select id="marketFilter" class="select2able" onchange="onChangeMarketAreas()">
                            <option value="" selected>ALL</option>
                        </select>
                    </div>

                    <div class="col-sm-2" style="z-index: 0;margin-bottom: 0px;">

                        <button onclick="getAllBots()" class="button" style="margin-top:10px;">Go</button>

                    </div>


                </div>

                <div class="col-lg-2" style="margin-bottom: 10px;">
                    <label>Global Search :</label>
                    <br />
                    <input type="text" id="searchBots" style="width: 80%;padding-bottom: 5px;padding-top: 5px;" class="inputTextStyle" placeholder="Type to search...">@*&nbsp;&nbsp; <button class="btn btn-primary">Search</button>*@
                </div>
            </div>
        </div>
    </div>
</div>

