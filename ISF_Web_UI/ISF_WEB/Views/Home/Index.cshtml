<style>
    .susan {
        border: 4px solid #CCC;
        width: 100px;
        height: 50px;
        display: block;
        position: absolute;
        left: 75%;
        top: 30%;
        margin: -25px 0 0 -50px;
        border-radius: 5px;
    }

    .icon-add, .icon-view {
        display: inherit;
    }

    th {
        border-bottom: 1px solid;
        border-color: lightgray;
    }

    tablebody {
        font: 14px/1.5 Lato, "Helvetica Neue", Helvetica, Arial, sans-serif;
        color: #777;
        font-weight: 300;
    }

    ul {
        list-style: none;
    }

    .quickLinks li {
        width: 250px;
        color: white;
        border: 1px solid;
        background-color: #666;
        opacity: .7;
        margin-top: 18px;
        cursor: pointer;
    }

        .quickLinks li:hover {
            opacity: 1;
            background-color: #333;
        }

    .widget-box {
        box-shadow: gray 1px 3px 16px;
    }

    .parent-panel {
        border: none;
        padding-top: 18px;
        min-height: 590px;
        background-color: #f3f3f3;
    }

    .panel-heading-custom {
        background: -webkit-gradient(linear, left top, left bottom, from(#6CA738), to(#88B916));
        color: white;
    }

    .panel-heading-custom-1 {
        background: -webkit-gradient(linear, left top, left bottom, from(#2b5799), to(#1a2f80));
        color: white;
    }

    .forTextOnHoverOnKnownIssue {
        position: relative;
        display: inline-block;
    }

        .forTextOnHoverOnKnownIssue .toolTipTextKnownIssue {
            visibility: hidden;
            color: #fff;
            text-align: center;
            border-radius: 6px;
            padding: 2px 0;
            width: 110px;
            font-size: small;
            height: 19px;
            background-color: #4a4242;
            color: #f9f9f9;
            /* Position the tooltip */
            position: absolute;
            z-index: 1;
        }

        .forTextOnHoverOnKnownIssue:hover .toolTipTextKnownIssue {
            visibility: visible;
        }
</style>
