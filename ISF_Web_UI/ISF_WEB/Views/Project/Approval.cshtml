
@{
    ViewBag.Title = "Project Approval";
}
@Scripts.Render("~/bundles/projectApproval")
@*@Scripts.Render("~/bundles/Project")*@

<script>
    $(document).ready(function () {
        getApprovalRequestBySignum();
    });


    var login_signum = signumGlobal /*"ebhjpra"*/;
