		//show bell icon to specific roles only
		$(document).ready(function () {

			// DR : 5, NE : 7, ASP : 14, ASP-DR : 15, PM : 1
			var sessionData = JSON.parse(ActiveProfileSession);

			if (sessionData != null && sessionData != undefined && (sessionData.roleID == 5 || sessionData.roleID == 7 || sessionData.roleID == 14 || sessionData.roleID == 15 || sessionData.roleID == 1)) {
				addBellIcon();
			}

		});

		//method to set bell icon
		function addBellIcon() {
			var NotificationData = document.getElementById('NotificationData').value;
			var img = document.createElement('img');
			img.id = "notificationImg";
			if (isNotificationReceived === true || isNotificationReceived === "true" || NotificationData !== "") {
				img.src = '../Content/images/notification-ring.svg';
			}
			else {
				img.src = '../Content/images/notification.svg';
			}
			document.getElementById('webNotifications').appendChild(img);
		}
